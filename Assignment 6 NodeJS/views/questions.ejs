<script>

    setInterval(displayHello, 1000);
        function displayHello() {
           
            document.getElementById("demo2").innerHTML -= 1;
            
            var totaltime = document.getElementById("demo2").innerHTML;
          var display = "";
          var minutes = Math.floor(totaltime/60)
    
          var seconds = totaltime - (minutes*60)
            if(seconds<10){
                display += ("Timer : " + minutes + ":0" + seconds)
            }
            else{
                display += ("Timer : " + minutes + ":" + seconds)
            }
          
          document.getElementById("demot").innerHTML = display;
            
          if (document.getElementById("demo2").innerHTML <= 300){
            document.getElementById("head").style.color = "Red";
          }
          
          if (document.getElementById("demo2").innerHTML <= 0){
            document.getElementById("subbutton").click();
          }
          
        }
        </script>

  
